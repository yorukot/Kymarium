# SMTP setting
SMTP_ENABLED=false
SMTP_HOST=example.com
SMTP_PORT=587
SMTP_USERNAME=example_user
SMTP_PASSWORD=password
SMTP_FROM=user@example.com

GOOGLE_CLIENT_ID=xxxxx-xxxxxxx.apps.googleusercontent.com
