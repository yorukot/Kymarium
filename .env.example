# APP_ENV can be dev or prod
APP_NAME=kymarium
APP_ENV=dev
APP_MACHINE_ID=1
APP_PORT=8000
APP_REGION="TW-Taipei"
APP_REGIONS="TW-Taipei,US-San Francisco"

FRONTEND_DOMAIN=localhost:5173
BACKEND_URL=http://localhost:8000

JWT_SECRET_KEY=thisisasecret

# Timescale setting
DB_HOST=localhost
DB_PORT=5432
DB_USER=kymarium
DB_PASSWORD=kymarium-this-is-a-really-long-password
DB_NAME=kymarium
DB_SSL_MODE=disable

# Redis setting
REDIS_HOST=localhost
REDIS_PORT=6379
